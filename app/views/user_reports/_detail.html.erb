<div class="modal-body">
  <%= form_for report, url: user_reports_path do |f| %>
    <%= f.label :title, t(".title"), class: "field-label" %>
    <%= f.text_field :title, class: "form-control field-input" %>

    <%= f.label :content, t(".content"), class: "field-label" %>
    <%= f.text_area :content, class: "form-control field-input" %>
  <% end %>
</div>
